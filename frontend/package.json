{
  "name": "heapundersink",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "postcss-run": "postcss ./src/imports.css --dir public --watch",
    "test": "jest --env=jsdom"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "async-mutex": "^0.3.2",
    "axios": "^0.26.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.2.2"
  },
  "devDependencies": {
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-redux": "^7.1.23",
    "@vitejs/plugin-react": "^1.0.7",
    "cssnano": "^5.1.5",
    "jest": "^27.5.1",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "postcss-preset-env": "^7.4.3",
    "ts-jest": "^27.1.4",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
